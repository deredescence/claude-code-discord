# Discord Bot Token (from Discord Developer Portal)
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Application ID
DISCORD_CLIENT_ID=your_client_id_here

# Optional: Restrict to specific guild for faster command registration
DISCORD_GUILD_ID=your_guild_id_here

# Claude Code executable path (optional, defaults to 'claude')
CLAUDE_PATH=claude

# Working directory for Claude Code sessions (optional)
CLAUDE_WORKDIR=
